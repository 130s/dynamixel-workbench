<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_workbench_pan_tilt_controllers" pkg="dynamixel_workbench_controllers" type="dynamixel_workbench_position_control" required="true" output="screen">
        <!-- <rosparam file="$(find dynamixel_workbench_monitor)/param/dynamixel.yaml" command="load"/> -->
        <rosparam>
          device_name_: "/dev/ttyUSB0"
          baud_rate_: 1000000
          motor_model_: "RX_28"
          protocol_version_: 1.0
          pan_motor_:
            motor_id_: 1
          tilt_motor_:
            motor_id_: 2
        </rosparam>
    </node>
</launch>
