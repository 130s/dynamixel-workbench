<!-- -*- mode: XML -*- -->

<launch>
    <node name="dynamixel_workbench_pan_tilt" pkg="dynamixel_workbench_controllers" type="dynamixel_workbench_position_control" required="true" output="screen">
        <!-- <rosparam file="$(find dynamixel_workbench_monitor)/param/dynamixel.yaml" command="load"/> -->
        <rosparam>
          device_name_: "/dev/ttyUSB0"
          baud_rate_: 1000000
          pan_motor_:
            motor_model_: "XM430_W210_R"
            motor_id_: 1
            protocol_version_: 2.0
          tilt_motor_:
            motor_model_: "XM430_W210_R"
            motor_id_: 2
            protocol_version_: 2.0
        </rosparam>
    </node>
</launch>
